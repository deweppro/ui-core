{
  "name": "ui-core",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href='https://deweppro.github.io/ui-core/'",
    "watch": "ng build --watch --configuration development",
    "build-lib": "ng build @deweppro/core",
    "pack-lib": "cd dist/deweppro/core && npm pack",
    "test": "ng test",
    "version-patch": "cd projects/deweppro/core && npm version patch",
    "version-minor": "cd projects/deweppro/core && npm version minor",
    "version-major": "cd projects/deweppro/core && npm version major",
    "publish": "npm run build-lib && npm run pack-lib && cd dist/deweppro/core && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.2",
    "@angular/common": "13.3.2",
    "@angular/compiler": "13.3.2",
    "@angular/core": "13.3.2",
    "@angular/forms": "13.3.2",
    "@angular/platform-browser": "13.3.2",
    "@angular/platform-browser-dynamic": "13.3.2",
    "@angular/router": "13.3.2",
    "@deweppro/core": "file:projects/deweppro/core",
    "normalize.css": "8.0.1",
    "rxjs": "^6.0.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.9",
    "@angular/cli": "13.3.2",
    "@angular/compiler-cli": "13.3.2",
    "@types/node": "17.0.23",
    "ng-packagr": "13.3.1",
    "typescript": "^4.0.0"
  }
}
